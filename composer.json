{
    "name": "deloittedigital/qd_satis",
    "description": "Simple static Composer repository generator for Quadrupal packages",
    "keywords": ["package", "repository", "generator", "static", "composer"],
    "homepage": "https://github.com/DeloitteDigitalAPAC/qd-satis",
    "type": "library",
    "license": "proprietary",
    "scripts": {
        "build": "@php vendor/bin/robo build",
        "serve": "@php -S localhost:8000 -t build"
    },
    "require": {
        "consolidation/robo": "^1.3",
        "composer/satis": "^1.0"
    }
}
